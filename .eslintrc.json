{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "class-methods-use-this": 0,
    "no-plusplus": 2,
    "react/forbid-prop-types": 0,
    "no-multi-assign": 0,
    "no-cond-assign": 0,
    "no-param-reassign": 0,
    "no-control-regex": 0,
    "camelcase":"warn",
    "no-nested-ternary": 0
  }
}
{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "class-methods-use-this": 0,
    "no-plusplus": 2,
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "import/prefer-default-export": "off"
  }
}